{
	"name": "Bootstrap",
	"author": "Francesco Siddi",
	"url": "https://github.com/blender/mediawiki-bootstrap",
	"descriptionmsg": "Bootstrap 4 based documentation-oriented skin.",
	"namemsg": "bootstrap",
	"license-name": "GPL-2.0+",
	"type": "skin",
	"ValidSkinNames": {
		"bootstrap": "Bootstrap"
	},
	"AutoloadClasses": {
		"SkinBootstrap": "Bootstrap.skin.php",
		"BootstrapTemplate": "BootstrapTemplate.php",
		"DeToc": "DeToc.php"
	},
	"MessagesDirs": {
		"Bootstrap": "i18n"
	},
	"ResourceModules": {
		"skins.bootstrap": {
				"styles": {
						"resources/vendor/bootstrap.min.css": { "media": "screen" },
						"resources/screen.css": { "media": "screen" },
						"resources/print.css": { "media": "print" }
				}
		},
		"skins.bootstrap.js": {
			"scripts": [
				"resources/vendor/bootstrap.bundle.min.js"
			],
			"position": "top"
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteSkinPath": "Bootstrap"
	},
	"manifest_version": 2
}
